<Window x:Class="Note_App.Notes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Note_App"
        mc:Ignorable="d"
        Title="Notes" Height="470" Width="290" ResizeMode="NoResize">
    <Grid Margin="0,0,0,0">
        <ListBox x:Name="list_Box" HorizontalAlignment="Center" Height="350" VerticalAlignment="Top" Width="274" d:ItemsSource="{d:SampleData ItemCount=5}" RenderTransformOrigin="0.446,0.492" Margin="0,10,0,0"/>
        <Button x:Name="ok_Button" Content="OK" HorizontalAlignment="Center" Height="58" Margin="0,368,0,0" VerticalAlignment="Top" Width="94" Click="ok_Button_Click" />

    </Grid>
</Window>
